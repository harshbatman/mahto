{
  "expo": {
    "name": "MAHTO",
    "slug": "mahto",
    "owner": "mahtobuild",
    "version": "1.1.16",
    "orientation": "portrait",
    "icon": "./assets/images/mahto-launcher-icon.png",
    "scheme": "ionizedvoid",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "bundleIdentifier": "com.harsh.mahto",
      "buildNumber": "16",
      "supportsTablet": true
    },
    "android": {
      "package": "com.harsh.mahto",
      "versionCode": 16,
      "versionName": "1.1.16",
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "backgroundColor": "#000000",
        "foregroundImage": "./assets/images/mahto-launcher-icon.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/mahto-icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/mahto-logo.png",
          "imageWidth": 180,
          "resizeMode": "contain",
          "backgroundColor": "#FFFFFF",
          "dark": {
            "backgroundColor": "#FFFFFF"
          }
        }
      ],
      [
        "expo-location",
        {
          "locationWhenInUsePermission": "MAHTO use your location to show nearby worekr, contractor and shops",
          "locationAlwaysAndWhenInUsePermission": "MAHTO use your location to show nearby worekr, contractor and shops",
          "isAndroidBackgroundLocationEnabled": false
        }
      ],
      "expo-image-picker"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "439e9b12-ec97-425d-aae0-6d90165239cd"
      }
    }
  }
}